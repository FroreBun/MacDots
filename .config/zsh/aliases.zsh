#  aliases 

# using bat instead of cat
alias cat="bat"

# directory aliases 

# using lsd instead of ls
alias ls="lsd"

# List directory contents
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'

# setting yazi to yy
alias yy="y"

# aliases to nvim to vim
alias vim="nvim"
